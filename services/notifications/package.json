{
  "name": "notifications",
  "version": "0.0.13",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --skipLibCheck",
    "plop": "plop",
    "generate-migration": "docker-compose run --rm notifications ./node_modules/.bin/typeorm migration:generate -f ./src/config/db-migration.config -n",
    "create-migrations": "docker-compose run --rm notifications ./node_modules/.bin/typeorm migration:create -f ./src/config/db-migration.config",
    "integration": "docker-compose run --rm notifications node ./node_modules/.bin/mocha \"src/integration-tests/**/*.spec.js\" --file=\"src/integration-tests/bootstrap.js\" --exit",
    "update-version": "git show origin/master:services/notifications/package.json | jq -r '.version' | perl -pe 's/^((\\d+\\.)*)(\\d+)(.*)$/$1.($3+1).$4/e' | xargs -L1 -I'{}' jq '.version=\"{}\"' package.json > package.json_tmp && mv package.json_tmp package.json"
  },
